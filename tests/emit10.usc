int main() {
    int factor = 10;
    int i = 0;
    int result;
    while (i < 3) {
        if (i < 2) {
            result = (factor * 2) + i;
        } else {
            result = (factor * 2) - i;
        }
        i = i + 1;
    }
    if (factor == 10){
    	i = 10;
    }
    printf("%d\n", result);
    return 0;
}

